{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "lib": [
      "ESNext"
    ],
    "allowJs": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "sourceMap": false,
    "baseUrl": ".",
    "types": ["node"]
  },
  "ts-node": {
    "swc": true,
    "esm": true,
    "experimentalSpecifierResolution": "node"
  },
  "exclude": [
    "node_modules/**/*",
  ]
}
