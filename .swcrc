{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true,
      "dynamicImport": true,
      "topLevelAwait": true
    },
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    },
    "experimental": {
      "plugins": [
        [
          "@swc/plugin-styled-components",
          {
            "displayName": true,
            "ssr": true
          }
        ]
      ]
    },
    "target": "esnext",
    "baseUrl": "."
  },
  "module": {
    "type": "nodenext"
  },
  "sourceMaps": true,
  "exclude": "node_modules/"
}
